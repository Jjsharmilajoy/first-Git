{
  "name": "TestRideVehicle",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "table": "test_ride_vehicles"
     }
   },
  "properties": {
    "id": {
      "id": true,
      "type": "string",
      "postgresql": {
        "dbDefault": "uuid_generate_v4()",
        "dataType": "uuid"
      }
    },
    "availability" : {
      "type" : "string",
      "postgresql": {
       "dataType": "character varying"
     }
    },
    "created_at": {
      "type": "date",
      "defaultFn": "now"
    },
    "updated_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "variants" : {
      "type": "belongsTo",
      "model": "Variant",
      "foreignKey": "variant_id",
      "postgresql": {
        "dataType": "uuid"
      }
    },
    "vehicle" : {
      "type": "belongsTo",
      "model": "Vehicle",
      "foreignKey": "vehicle_id",
      "postgresql": {
        "dataType": "uuid"
      }
    },
    "customer_user" : {
      "type": "belongsTo",
      "model": "Users",
      "foreignKey": "customer_user_id",
      "postgresql": {
        "dataType": "uuid"
      }
    },
    "dealer" : {
      "type": "belongsTo",
      "model": "Dealer",
      "foreignKey": "dealer_id",
      "postgresql": {
        "dataType": "uuid"
      }
    },
    "manufacturer" : {
      "type": "belongsTo",
      "model": "Manufacturer",
      "foreignKey": "manufacturer_id",
      "postgresql": {
        "dataType": "uuid"
      }
    }
  },
  "acls": [],
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": []
    }
  },
  "methods": []
}
