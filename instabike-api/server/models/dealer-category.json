{
  "name": "DealerCategory",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "postgresql": {
      "table": "dealer_categories"
    }
  },
  "properties": {
    "id": {
      "id": true,
      "type": "string",
      "postgresql": {
        "dbDefault": "uuid_generate_v4()",
        "dataType": "uuid"
      }
    },
    "name" : {
      "type" : "string",
      "required" : true,
      "postgresql": {
        "dataType": "character varying"
      }
    },
    "color_code" : {
      "type" : "string",
      "postgresql": {
        "dataType": "character varying"
      }
    },
    "created_at": {
      "type": "date",
      "defaultFn": "now"
    },
    "updated_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "manufacturer": {
      "type": "belongsTo",
      "model": "Manufacturer",
      "foreignKey": "manufacturer_id"
    },
    "dealers": {
      "type": "hasMany",
      "model": "Dealer",
      "foreignKey": "dealer_category_id",
      "postgresql": {
        "dataType": "uuid"
      }
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property" : ["find"]
    }
  ],
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": []
    }
  },
  "methods": []
}
