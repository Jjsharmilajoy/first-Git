{
  "name": "VehicleFeature",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "table": "vehicle_features"
     }
   },
  "properties": {
    "id": {
      "id": true,
      "type": "string",
      "postgresql": {
        "dbDefault": "uuid_generate_v4()",
        "dataType": "uuid"
      }
    },
    "name" : {
      "type" : "string",
      "required" : true,
      "postgresql": {
       "dataType": "character varying"
     }
    },
    "description" : {
      "type" : "string",
      "postgresql": {
       "dataType": "character varying"
     }
    },
    "image_url" : {
      "type" : "string",
      "postgresql": {
        "dataType": "character varying"
      }
    },
    "created_at": {
      "type": "date",
      "defaultFn": "now"
    },
    "updated_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "vehicle" : {
      "type": "belongsTo",
      "model": "Vehicle",
      "foreignKey": "vehicle_id",
      "postgresql": {
        "dataType": "uuid"
      }
    },
    "variant" : {
      "type": "belongsTo",
      "model": "Variant",
      "foreignKey": "variant_id",
      "postgresql": {
        "dataType": "uuid"
      }
    },
    "manufacturer" : {
      "type": "belongsTo",
      "model": "Manufacturer",
      "foreignKey": "manufacturer_id",
      "postgresql": {
        "dataType": "uuid"
      }
    }
  },
  "acls": [],
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": []
    }
  },
  "methods": []
}
