{
  "name": "ManufacturerFinancierOrder",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "postgresql": {
      "table": "manufacturer_financier_orders"
    }
  },
  "properties": {
    "id": {
      "id": true,
      "type": "string",
      "postgresql": {
        "dbDefault": "uuid_generate_v4()",
        "dataType": "uuid"
      }
    },
    "order": {
      "type": "number"
    },
    "created_at": {
      "type": "date",
      "defaultFn": "now"
    },
    "updated_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "manufacturer" : {
      "type": "belongsTo",
      "model": "Manufacturer",
      "foreignKey": "manufacturer_id"
    },
    "financier" : {
      "type": "belongsTo",
      "model": "Financier",
      "foreignKey": "financier_id"
    },
    "city" : {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": "city_id"
    },
    "state" : {
      "type": "belongsTo",
      "model": "State",
      "foreignKey": "state_id"
    },
    "zone" : {
      "type": "belongsTo",
      "model": "Zone",
      "foreignKey": "zone_id"
    },
    "country" : {
      "type": "belongsTo",
      "model": "Country",
      "foreignKey": "country_id"
    }
  },
  "acls": [],
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": []
    }
  },
  "methods": []
}
