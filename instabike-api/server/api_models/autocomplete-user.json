{
  "name": "AutocompleteUsers",
  "base": "PersistedModel",
  "properties": {
    "searchField": {
      "type" : "string",
      "required" : true
    },
    "searchValue": {
      "type" : "string",
      "required" : true
    },
    "limit": {
      "type" : "number",
      "required" : true,
      "default": 10
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": []
    }
  }
}
