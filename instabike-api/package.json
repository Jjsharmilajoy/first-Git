{
  "name": "instabike-control",
  "docker_container_name": "api",
  "version": "0.0.2",
  "main": "server/server.js",
  "engines": {
    "node": "8.10.0"
  },
  "scripts": {
    "lint": "eslint .",
    "doc": "jsdoc server/services/*.js -d code-flow",
    "dev": "nodemon --exec node server/server.js",
    "prod": "node server/server.js",
    "pretest": "npm run autoupdate-test",
    "test": "NODE_ENV=test istanbul cover _mocha $(find -name \"*.spec.js\" -not -path \"./node_modules/*\")",
    "posttest": "npm run integration-test",
    "preunit-test": "npm run autoupdate-test",
    "unit-test": "NODE_ENV=test istanbul cover _mocha $(find -name \"*.spec.js\" -not -path \"./node_modules/*\")",
    "postunit-test": "npm run lint && nsp check",
    "preintegration-test": "npm run autoupdate-test",
    "integration-test": "NODE_ENV=test mocha $(find -name \"*.test.js\" -not -path \"./node_modules/*\")",
    "postintegration-test": "npm run lint && nsp check",
    "precommit": "npm run lint",
    "precommit_TODO": "docker-compose run --rm api npm run test",
    "console": "loopback-console",
    "autoupdate": "node server/autoupdate.js",
    "autoupdate-test": "NODE_ENV=test node server/autoupdate.js",
    "postautoupdate-test": "NODE_ENV=test node server/db_migrate.js",
    "postautoupdate": "node server/db_migrate.js",
    "dbMigrate": "node server/db_migrate.js",
    "migrationGenerator": "node server/generators/migrationGenerator.js",
    "setup_meta_data": "node server/importers/meta_data_importer.js",
    "setup_manufacturer": "node server/importers/manufacturer.js",
    "setup_prod_suzuki": "node server/importers/pre_prod_suzuki_manufacturer.js",
    "setup_abc_auto": "node server/importers/abc_auto_manufacturer.js",
    "setup_honda": "node server/importers/honda_manufacturer.js",
    "setup_tvs": "node server/importers/tvs_manufacturer.js",
    "setup_hero": "node server/importers/hero_manufacturer.js",
    "setup_bajaj": "node server/importers/bajaj_manufacturer.js",
    "setup_yamaha": "node server/importers/yamaha_manufacturer.js",
    "setup_suzuki": "node server/importers/suzuki_manufacturer.js",
    "setup_similar_vehicles": "node server/importers/similar_vehicle.js",
    "setup_abc_auto_similar_vehicles": "node server/importers/abc_auto_similar_vehicle.js",
    "setup_financiers": "node server/importers/financier_importer.js",
    "setup_exchange_vehicles": "node server/importers/exchange_vehicle.js",
    "setup_test_data": "node server/importers/test_data_importer.js",
    "setup_demo_data": "node server/importers/demo_data_importer.js",
    "setup_suzuki_variant": "node server/importers/suzuki_variant.js",
    "validate": "npm ls",
    "report_job": "node server/services/report_job_service.js",
    "reminder_job": "node server/services/reminder_job_service.js",
    "missed_job": "node server/services/missed_job_service.js",
    "mis_report_job": "node server/services/MIS_report_job_service.js",
    "delete_duplicate_accessories": "node server/services/delete_duplicate_accessory.js",
    "setup_financier_leads": "node server/lead_to_financier_lead/script.js",
    "followups_reminder_job": "node server/services/followups_reminder_job_service.js"
  },
